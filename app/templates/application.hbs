{{#paper-nav-container class="site-nav-container"}}
  {{#paper-sidenav class="md-sidenav-left md-whiteframe-z2" locked-open="gt-sm"}}
    {{#paper-toolbar}}
      <div class="md-toolbar-tools">
        <div class="logo">
          {{#link-to "index"}}
            <img src="images/ember-logo-white.png" height="30">&nbsp;&nbsp;<strong>Ec</strong>
          {{/link-to}}
        </div>
      </div>
    {{/paper-toolbar}}
    {{#paper-content}}
      {{#paper-list}}
        {{#paper-item action="transitionTo" param="selectBox"}}
          {{paper-icon icon="select-all"}}
          Select Box
        {{/paper-item}}
      {{/paper-list}}
    {{/paper-content}}
  {{/paper-sidenav}}

  <div layout="column" tabindex="-1" role="main" flex="">
    {{#paper-toolbar}}
    {{/paper-toolbar}}
    {{#paper-content}}
      <div class="page">
        {{outlet}}
      </div>
    {{/paper-content}}
  </div>
{{/paper-nav-container}}
